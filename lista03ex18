using System;
/*Construa um programa que apresente a tabuada de um número N. O valor de N será apresentado pelo usuário. */
class Program {
  public static void Main (string[] args) {
    int num, tb;
    
    for(int i=1;i==10;i++){
      Console.WriteLine("tabudada do : ",i);
      num=int.Parse(Console.ReadLine());
      tb=num*i;
      Console.WriteLine("{0}*{1} = {2}", num , i , tb);
    }
  }
}