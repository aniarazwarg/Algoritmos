using System;
/*Construa um algoritmo que tendo como dados de entrada o preço de um produto (PR) e seu código de origem (CO), apresente o preço e a sua procedência, de acordo com a tabela abaixo:

Código de Origem (CO) /Procedencia:
1 Sul 
2 Sudeste 
3 Centro-Oeste 
4 Norte 
5 Nordeste 
*/
class Program {
  public static void Main (string[] args) {
 string CO;
    double PR ;
    Console.Write ("Digite o valor do produto: R$");
    PR=double.Parse(Console.ReadLine()); 
   Console.Write ("Digite o código da origem: ");
    CO=Console.ReadLine(); 
    switch (CO) 
    {
      case"1":
      Console.WriteLine("O valor do produto é : {0}, e sua procedencia é : Sul.",PR);
      break;
        
       case"2":
      Console.WriteLine("O valor do produto é : {0}, e sua procedencia é : Sudeste.",PR);
      break;     
      case"3":
      Console.WriteLine("O valor do produto é : {0}, e sua procedencia é : Centro-Oeste.",PR);
      break;      
      case"4":  
      Console.WriteLine("O valor do produto é : {0}, e sua procedencia é : Norte.",PR);
      break;      
      case"5":
      Console.WriteLine("O valor do produto é : {0}, e sua procedencia é : Nordeste.}",PR);
      break;
      default: 
      break;
  }
}
}